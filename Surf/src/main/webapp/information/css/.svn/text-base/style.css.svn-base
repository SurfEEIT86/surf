.model-box-wrap .this-modelC .pic-wrap img { }
.model-box-wrap .this-modelC .pic-wrap span.note { float:none; display: block; text-align: right; }

/*fixbox*/
.fix-box { position: fixed; right:0px; bottom:100px; z-index: 100000; }
.fix-box .btn-wrap { width:40px; float:left;  }
.fix-box .btn-wrap .fix-btn { background: rgba(55, 133, 133, 0.8); padding:10px; text-align: center; color:#fff; font-weight: bold; font-size: 18px; 
  -moz-border-top-left-radius: 5px; /* Firefox */  
  -webkit-border-top-left-radius: 5px; /* Safari, Chrome */  
  -khtml-border-top-left-radius: 5px; /* Konqueror */  
  border-top-left-radius: 5px; /* Opera 10.50 and later (CSS3 standard) */
}
.fix-box .btn-wrap>a { display: block;background: rgba(55, 133, 133, 0.5); padding:9px 0; text-align: center; color:#fff; font-weight: bold; font-size: 14px;
 -moz-border-bottom-left-radius: 5px; /* Firefox */  
  -webkit-border-bottom-left-radius: 5px; /* Safari, Chrome */  
  -khtml-border-bottom-left-radius: 5px; /* Konqueror */  
  border-bottom-left-radius: 5px; /* Opera 10.50 and later (CSS3 standard) */
}
.fix-box .btn-wrap>a i { font-size: 18px;  }
.fix-box .fix-content-wrap { float:left;max-width:400px; min-height: 203px;}
.fix-box .fix-content-wrap { background: #fff; }
.fix-box .fix-content-wrap ol {  padding:8px 17px;     margin: 0;}
.fix-box .fix-content-wrap ol li { /*list-style-type:decimal-leading-zero;  list-style-position: inside; display: list-item;*/ line-height: 30px; border-bottom:1px solid #E0E0E0; width:auto!important; float:none!important;   }
.fix-box .fix-content-wrap ol li:last-of-type { border:none; }
.history-arrP , .history-arrN { color: #B6B6B6; text-align: center; cursor: pointer;}
.history-arrP { margin-bottom:10px; }
.history-arrN{    margin-top: 10px;}
.history-arrP:hover , .history-arrN:hover { color: #878787;}
