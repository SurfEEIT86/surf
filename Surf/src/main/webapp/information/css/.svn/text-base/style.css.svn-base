    padding-bottom: 6px;}

/*======================@0 form start ==========================*/
input[type="radio"]{
    display:none;
}
input[type="radio"] + label span{
    display:inline-block;
    width:20px;
    height:20px;
    cursor:pointer;
    margin: 1px 5px 0 0;
    background: url(../images/icon/inputbtn.png) no-repeat right center;
    background-size: 41px 22px;
}
input[type="radio"]:checked + label span{
    background: url(../images/icon/inputbtn.png) no-repeat left center;
    background-size: 41px 22px;
}
.pav-fast-search input[type="radio"] + label .inputpic { 
  width:60px;
    height:60px;
    cursor:pointer;
    margin-bottom:15px;
}
.pav-fast-search input[type="radio"] + label .inputpicA { 
    background: url(../images/icon/percent.png) no-repeat left center;
    background-size: 180px 60px;
}
.pav-fast-search input[type="radio"] + label .inputpicB { 
    background: url(../images/icon/percent.png) no-repeat center center;
    background-size: 180px 60px;
}
.pav-fast-search input[type="radio"] + label .inputpicC { 
    background: url(../images/icon/percent.png) no-repeat right center;
    background-size: 180px 60px;
}

/*======================@0 form start ==========================*/

/*======================@1 common start ==========================*/
.default-img-wrap { text-align: center; }
.note-text { font-size:12px; }
.base-box { margin-bottom:20px; }
.base-box .box-head { padding:0px .3em; line-height: 50px; background: #368585; font-size: 20px; color:#fff; font-weight: bold;} /*基本上綠下白box*/
.base-box .box-head span { vertical-align: middle; }
.base-box .box-head .Switching { float:right; display:inline-block; font-size:16px; color: #fff; background-color: #2D7676; padding: 0 15px; }
.base-box .box-head .Switching:hover {font-size:17px; color: #7EC2C2; background-color: #1E6565;}
.base-box .box-head .Switching:hover i { 
    -webkit-transition: all 0.3s ease;
  -moz-transition: all 0.3s ease;
  -o-transition: all 0.3s ease;
  transition: all 0.3s ease;
    transform: rotate(-270deg);
    -webkit-transform: rotate(360deg);
    -moz-transform: rotate(360deg);
    -ms-transform: rotate(360deg);
    -o-transform: rotate(360deg); 
}
.base-box .box-content:after { content:''; display: block; clear:both; }
.base-shadow { /*白色底+陰影*/
  overflow: hidden;
  background: #fff; 
  -webkit-box-shadow: 0px 0px 5px 0px #DFDFDF;
  -moz-box-shadow: 0px 0px 5px 0px #DFDFDF;
  box-shadow: 0px 0px 5px 0px #DFDFDF;

  -moz-border-radius: 2px; /* Firefox */  
  -webkit-border-radius: 2px; /* Safari, Chrome */  
  -khtml-border-radius: 2px; /* Konqueror */  
  border-radius: 2px; /* Opera 10.50 and later (CSS3 standard) */ 
}

.aside-l { padding-left:0; padding-right:20px;}
.aside-r {padding-right:0; padding-left:20px; }
.center-content { padding:0; }

span.NoResult {
    padding: 0 20px;
    color: #494949;
}

/*列表前的灰點點*/
ul.list-style-dot li {  list-style-position: inside; list-style-type: initial;   color:#DBDBDB; font-size: 30px;  }
ul.list-style-dot li a.a1 { margin-left: -4px; }

/*紅色漸層背景按鈕*/
.redbtn {
  font-weight: bold;
  border:none;
  color:#fff;
  /* Permalink - use to edit and share this gradient: http://colorzilla.com/gradient-editor/#ee503d+0,cc3a2a+81 */
  background: rgb(238,80,61); /* Old browsers */
  /* IE9 SVG, needs conditional override of 'filter' to 'none' */
  background: url(data:image/svg+xml;base64,);
  background: -moz-linear-gradient(top,  rgba(238,80,61,1) 0%, rgba(204,58,42,1) 81%); /* FF3.6+ */
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(238,80,61,1)), color-stop(81%,rgba(204,58,42,1))); /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(top,  rgba(238,80,61,1) 0%,rgba(204,58,42,1) 81%); /* Chrome10+,Safari5.1+ */
  background: -o-linear-gradient(top,  rgba(238,80,61,1) 0%,rgba(204,58,42,1) 81%); /* Opera 11.10+ */
  background: -ms-linear-gradient(top,  rgba(238,80,61,1) 0%,rgba(204,58,42,1) 81%); /* IE10+ */
  background: linear-gradient(to bottom,  rgba(238,80,61,1) 0%,rgba(204,58,42,1) 81%); /* W3C */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ee503d', endColorstr='#cc3a2a',GradientType=0 ); /* IE6-8 */
}
.redbtn:hover { opacity: .9; }

/*內頁共用*/
.page-wrap .page-head { padding-top:35px; }
.page-wrap .page-head .title {width:70%; float:left; margin:0; padding:0 20px;}
.page-wrap .page-head .title span.big {display: block; float:left; vertical-align: middle; font-size: 28px; font-weight: bold;}
.page-wrap .page-head .title span.sm { display: block; float:left; line-height: 40px; font-weight: bold; font-size: 20px; vertical-align: middle;  }
.page-wrap .page-head .title .i-title { display: inline-block; vertical-align: middle; width:50px; height:50px; background: url(../images/icon/icon3.png) no-repeat; background-position: left center; background-size: auto 50px;  }
.page-wrap .page-head .i-title-wrap { padding: 0 10px 0 20px; }
.page-wrap .page-content .center-title { padding:3px 9px;  width:auto; font-size:28px; font-weight: bold; border-bottom:3px solid #368585; }
.page-kw-search { width:30%; float:right;padding:0 20px; }
.path { font-size: 14px; padding:10px 20px; clear:both; background: #FAFAFA; border-top:1px solid #ededed; border-bottom:1px solid #ededed; }

/*白底假頁籤按鈕*/
