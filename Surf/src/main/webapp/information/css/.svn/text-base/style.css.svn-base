.hot-article .content-wrap h3:hover {     color: #FF8779;}
.hot-article .content-wrap .date {color:#a0a0a0; font-size:13px; margin:7px 0;}
.hot-article .content-wrap .date i { margin-right:5px; }
.hot-article .content-wrap .text { overflow : hidden;   }

.pic-list:after { display: block; clear:both; content:''; }
.pic-list li { width:49.5%; float:left; background: #fff; padding:10px;  margin-bottom:10px;}
.pic-list li:nth-of-type(odd) {margin-right:1%;}
.pic-list .img-wrap { padding-right:5px;  width:30%; float:left;}
.pic-list .content-wrap { padding-left:5px;  width:70%; float:left;}
.pic-list .content-wrap h3 { margin:10px 0; color:#368585; font-size: 18px; overflow : hidden;  text-overflow : ellipsis;  white-space : nowrap;  }
.pic-list .content-wrap .text { overflow : hidden; line-height: 24px;   height: 48px; color:#111;  }
.pic-list li:hover { background: #acc71f; }
.pic-list li:hover .content-wrap h3, .pic-list li:hover .content-wrap .text { color:#fff; }

.article-list tr td.col-title { width:auto; }
.article-list tr td.col-title a { color:#368585; width:100%; display: block; height:auto;}
.article-list tr td.col-content {  width: 140px; text-align: center; border-left: 1px solid #DFDFDF; }
.article-list tr td.col-content a { padding:0; color:#111; }
.article-list tr:hover td a { color:#fff; }
.article-list tr:hover td.col-content {border-left: 1px solid #fff; }

.blog-dt-content { padding:20px; overflow: visible; }
.blog-dt-content .box-head { border-bottom:1px solid #d3d3d3; }
.blog-dt-content .box-head h1 { margin:0 0 5px 0; font-size: 28px; font-weight: bold; }
.blog-dt-content .box-head ul li { float:left; padding-right:10px; color:#999; }
.blog-dt-content .box-head .share { margin-top:5px; }
.blog-dt-content .box-bottom { position: relative; border-top:1px solid #d8d8d8; margin:30px 0 5px 0;}
.blog-dt-content .box-bottom span { font-size: 18px; background: #fff; padding:0 10px; position: absolute; top:-12px; left:30px; color:#666;}

.fb-share-button {    vertical-align: top;}
.fb-like { margin: 2px 0 0 5px; }

.p-blog-wrap .pagi { padding:0; }
.p-blog-wrap .pagi .btn { font-size: 30px; }
.p-blog-wrap .pagi .btn:hover { color:#acc71f; }
.p-blog-wrap .pagi .title { color:#368585; font-size: 18px; padding:0 10px;}

.relateblog-wrap { padding:20px; }
.relateblog-wrap .box-head { background: #000; }
.relateblog-wrap .box-head span { margin-left:20px; display: block; width:143px; font-size: 20px;  text-align: center; background: url(../images/relateblog-bg.png) no-repeat; background-size: 143px; }
.relateblog-wrap ul {margin:10px 0 0 10px;}
.relateblog-wrap ul li {     line-height: 32px; overflow : hidden; text-overflow : ellipsis; white-space : nowrap; list-style-type: disc; list-style-position: inside; color:#D8D8D8; font-size: 27px;}
.relateblog-wrap ul li a { font-size: 18px; }
/*======================== blog end ===============*/

/*=========================sitemap====================*/
.p-sitemap-wrap .page-content { padding:20px; padding-top:0; } 
.p-sitemap-wrap .this-item { border-bottom:1px solid #d8d8d8; margin-top:20px; padding-bottom:20px; }
.p-sitemap-wrap .this-item:last-of-type { border:none; padding-bottom:0; }
.p-sitemap-wrap .this-item h1 { font-size: 22px; font-weight: bold; color:#368585;     margin: 0 0 10px 0;}
.p-sitemap-wrap .this-item ul.text li { float:left;    margin-right: 30px; line-height: 30px;}
.p-sitemap-wrap .this-item .categ .title { border-right:1px solid #EF503D; padding-right:20px;} 
.p-sitemap-wrap .this-item .categ .content { padding-left:20px; }
.p-sitemap-wrap .this-item .categ-wrap { padding-bottom: 14px; }
.p-sitemap-wrap .this-item a {color:#434343;}
.p-sitemap-wrap .this-item a:hover {color:#acc71f;}
/*=========================sitemap end====================*/

/*=========================contact====================*/
.contact-info h2 { color:#EF503D; margin:5px 0 20px 0 ; }
.contact-info p { font-size: 18px; line-height: 28px; color:#444;}
.contact-info i.fa-envelope-o { margin:10px; }

.contact-center { padding:30px 20px; margin:0;}
.contact-form input[type="text"], .contact-form textarea{
  padding: 10px;
  border: 1px solid #bababa;
  width:70%;
  background: #fff;
  color:#111;
  width:100%;
  -webkit-box-shadow: inset 0px 1px 3px 0px #d8d8d8;
  -moz-box-shadow: inset 0px 1px 3px 0px #d8d8d8;
  box-shadow: inset 0px 1px 3px 0px #d8d8d8;

  -webkit-transition: box-shadow 0.3s;
  -moz-transition: box-shadow 0.3s;
  -o-transition: box-shadow 0.3s;
  transition: box-shadow 0.3s;
}
.contact-form input:focus, .contact-form textarea:focus { border: 1px solid rgba(54, 133, 133, 0.83);
-webkit-box-shadow: 0 1px 5px rgba(54, 133, 133, 0.83);
-moz-box-shadow: 0 1px 5px rgba(54, 133, 133, 0.83);
box-shadow: 0 1px 5px rgba(54, 133, 133, 0.83);
outline: none;

  }

.contact-form label { color: #111; font-weight: normal; }
.contact-form { 
  padding:30px 0; 
  background: #f9f9f9;
  border:1px solid #e9e9e9;
  -moz-border-radius: 3px;
  -webkit-border-radius: 3px;
  -khtml-border-radius: 3px; 
  border-radius: 3px;
}
.contact-form table { width: 100%; margin:0 auto; }
.contact-form tr td { padding:5px 8px; }
.contact-form tr td:nth-of-type(1) { text-align: right; }
.contact-form tr td:nth-of-type(2) { padding-right:40px; }
.contact-form .group-captcha input { width:70%; float:left;}
.contact-form .group-captcha .captcha {  width:30%; vertical-align: middle; float:left; padding-left:8px; }
.contact-form .action-button { padding-right:40px; }
.contact-form .action-button button {
  padding: 8px 35px;
  font-size: 18px;
  font-weight: 600;
  cursor: pointer;
  -webkit-transition: all 0.5s ease;
  -moz-transition: all 0.5s ease;
  -o-transition: all 0.5s ease;
  transition: all 0.5s ease; 
  clear:both; 
}
.contact-form  span.must { color:red!important; font-size: 16px; margin:0 5px; }
/*============================contact end=========================*/

